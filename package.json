{
  "name": "ota-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "babel-node src/bin/www",
    "server:prod": "node dist/bin/www",
    "dev": "DEBUG=ota-server::* NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel src --out-dir dist --copy-files",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~4.3.4",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^2.0.20"
  }
}
